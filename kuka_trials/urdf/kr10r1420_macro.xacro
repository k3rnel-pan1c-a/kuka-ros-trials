<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:macro name="kuka_kr10r1420" params="prefix">
    <xacro:include filename="$(find kuka_trials)/urdf/common_materials.xacro"/>
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/base_link_v.stl" />
        </geometry>
        <xacro:material_kuka_black />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/base_link_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="320.0"/>
        <inertia ixx="10.0" ixy="0.0" ixz="0.0" iyy="12.0" iyz="0.0" izz="5.0"/>
      </inertial>
    </link>

    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_1_v.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_1_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="20.0"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.2" iyz="0.0" izz="0.8"/>
      </inertial>
    </link>

    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_2_v.stl"/>
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_2_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />

        <mass value="18.0"/>
        <inertia ixx="0.62" ixy="0.0" ixz="0.0" iyy="0.54" iyz="0.0" izz="0.35"/>
      </inertial>
    </link>

    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_3_v.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_3_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="15.0"/>
        <inertia ixx="0.40" ixy="0.0" ixz="0.0" iyy="0.35" iyz="0.0" izz="0.25"/>
      </inertial>

    </link>

    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_4_v.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_4_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="9.0"/>
        <inertia ixx="0.18" ixy="0.0" ixz="0.0" iyy="0.16" iyz="0.0" izz="0.10"/>
      </inertial>

    </link>

    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_5_v.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_5_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>

        <mass value="6.0"/>

        <inertia ixx="0.065" ixy="0.0" ixz="0.0" iyy="0.060" iyz="0.0" izz="0.040"/>
      </inertial>
    </link>

    <link name="${prefix}link_6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_6_v.stl" />
        </geometry>
        <xacro:material_kuka_pedestal />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_trials/meshes/link_6_c.stl" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="4.0"/>
        <inertia ixx="0.030" ixy="0.0" ixz="0.0" iyy="0.028" iyz="0.0" izz="0.020"/>
      </inertial>
    </link>

    <!-- Following REP199, this frame shall be use to attach EEF or other equipment -->
    <link name="${prefix}flange">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.80"/>
        <inertia ixx="0.0006" ixy="0.0" ixz="0.0" iyy="0.0006" iyz="0.0" izz="0.0008"/>
      </inertial>
    </link>

    <joint name="${prefix}joint_a1" type="revolute">
      <origin xyz="0 0 0.450" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 -1"/>
      <limit effort="500" lower="${radians(-170)}" upper="${radians(170)}" velocity="${radians(220)}"/>
    </joint>
    <joint name="${prefix}joint_a2" type="revolute">
      <origin xyz="0.150 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 1 0"/>
      <limit effort="500" lower="${radians(-185)}" upper="${radians(65)}" velocity="${radians(210)}"/>
    </joint>
    <joint name="${prefix}joint_a3" type="revolute">
      <origin xyz="0.610 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 1 0"/>
      <limit effort="500" lower="${radians(-137)}" upper="${radians(163)}" velocity="${radians(270)}"/>
    </joint>
    <joint name="${prefix}joint_a4" type="revolute">
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="-1 0 0"/>
      <limit effort="500" lower="${radians(-185)}" upper="${radians(185)}" velocity="${radians(381)}"/>
    </joint>
    <joint name="${prefix}joint_a5" type="revolute">
      <origin xyz="0.660 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 1 0"/>
      <limit effort="500" lower="${radians(-120)}" upper="${radians(120)}" velocity="${radians(311)}"/>
    </joint>
    <joint name="${prefix}joint_a6" type="revolute">
      <origin xyz="0.080 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="-1 0 0"/>
      <limit effort="500" lower="${radians(-350)}" upper="${radians(350)}" velocity="${radians(492)}"/>
    </joint>
    <joint name="${prefix}joint_a6-flange" type="fixed">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>
  </xacro:macro>
</robot>
